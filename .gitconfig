[alias]
  ls = log --pretty=format:"%C(yellow)%h\\ %ad%Cgreen%d\\ %Creset%s%C(cyan)\\ [%cn]" --decorate --date=short
  ll = log --pretty=format:"%C(yellow)%h\\ %ad%Cgreen%d\\ %Creset%s%C(cyan)\\ [%cn]" --decorate --numstat --date=short
  lt = log --graph --oneline --decorate --all
  la = "!git config -l | grep alias | cut -c 7-"
  cp = cherry-pick
  st = status
  cl = clone
  ci = commit
  cim = comit --amend
  cimne = commit --amend --no-edit
  co = checkout
  cob = checkout -b	
  br = branch 
  diff = diff --word-diff
  dc = diff --cached
  sl = stash list
  sa = stash apply
  ss = stash save
  rh = reset --hard
  rb = rebase
  rbc = rebase --continue
  rba = rebase --abort
  rbi = rebase --interactive
  m = merge --no-ff
  pr = pull --rebase
  ps = push
  psf = push -f
  f = fetch

[user]
  name = #WEBER Logan
  email = #weberlogan@gmail.com

[push]
  default = simple

[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol

